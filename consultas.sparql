    ?x dbpedia-owl:isPartOf ?y .
    ?y rdfs:label ?statename FILTER(LANG(?statename ) = "" || LANGMATCHES(LANG(?statename ), "en")) .
    FILTER ( REGEX(STR(?cityname), "Santos", "i")  )
